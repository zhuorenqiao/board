{
  "name": "board",
  "description": "yatian-end-ststemOS",
  "keywords": [
    "jquery",
    "swiper",
    "iCheck",
    "animate",
    "summernote",
    "footable"
  ],
  "dependencies": {
    "swiper": "~ 3.4.1",
    "PACE": "pace#^1.0.2",
    "footable": "^3.1.4",
    "font-awesome": "~4.7.0",
    "animate.css": "~3.5.1",
    "bootstrap": "^3.3.7",
    "artTemplate": "^3.0.1",
    "bootstrap-datepicker": "^1.6.4",
    "summernote": "^0.8.2",
    "jquery-slimscroll": "jquery.slimscroll#^1.3.8",
    "metisMenu": "2.0.2",
    "bootstrap-paginator": "^1.0.2",
    "layer": "^3.0.3",
    "jquery-validation": "^1.16.0"
  },
  "overrides": {
    "bootstrap": {
