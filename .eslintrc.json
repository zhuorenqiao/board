{
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "jquery": true
    },
  "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": 0,
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
      "no-alert": 1,
      "no-undef": 1,
      "no-console": 1,
      "no-unused-vars": 1,
      "no-unused-vars": 1
    }
}
